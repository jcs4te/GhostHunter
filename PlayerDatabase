package edu.virginia.cs2110.ghosthunter;

import java.util.Map;
import java.util.TreeMap;



public class PlayerDatabase {
	public TreeMap<String, Hunter> database; 
	
	public PlayerDatabase() {
		database = new TreeMap<String, Hunter>(); 
	}
	public void addPlayer(Hunter h) {
		database.put(h.getUsername(), h); 
	}
	
	PlayerDatabase allPlayers = new PlayerDatabase(); 

}
